{
  "version": 1,
  "formatters": {
    "simple": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": [
      "console"
    ]
  },
  "loggers": {
    "dry_pipe.janitors": {
      "level": "DEBUG",
      "handlers": ["console"]
    },
    "dry_pipe.janitors.main_d": {
      "level": "DEBUG",
      "handlers": ["console"]
    },
    "dry_pipe.janitors.download_d": {
      "level": "DEBUG",
      "handlers": ["console"]
    }
  }
}